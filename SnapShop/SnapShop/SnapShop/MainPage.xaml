<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:SnapShop"
             x:Class="SnapShop.MainPage" BarBackgroundColor="Orange">

    <NavigationPage Title="Capture" BarBackgroundColor="Orange">
        <x:Arguments>
            <local:CapturePage/>
        </x:Arguments>
    </NavigationPage>

    <NavigationPage Title="History" BarBackgroundColor="Orange">
        <x:Arguments>
            <local:HistoryPage/>
        </x:Arguments>
    </NavigationPage>
    
    

</TabbedPage>